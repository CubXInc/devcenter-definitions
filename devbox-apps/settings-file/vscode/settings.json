{
    "editor.accessibilitySupport": "on",
    "editor.inlineSuggest.enabled": true,
    "editor.screenReaderAnnounceInlineSuggestion": true,
    "editor.inlineCompletionsAccessibilityVerbose": true,
    "accessibility.verbosity.inlineCompletions": false,
    "settingsSync.ignoredExtensions": [
        "ms-toolsai.datawrangler"
    ],
    "accessibility.signals.format": {
        "sound": "never",
        "announcement": "never"
    },
    "git.autofetch": true,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.startupEditor": "none",
    "editor.hover.delay": 5000,
    "editor.quickSuggestions": {
        "other": "off"
    },
    "editor.suggestOnTriggerCharacters": false,
    "accessibility.signals.chatRequestSent": {
        "announcement": "off"
    },
    "accessibility.signals.save": {
        "sound": "always"
    },
    "auto-close-tag.excludedTags": [
        "area",
        "base",
        "br",
        "col",
        "command",
        "embed",
        "hr",
        "img",
        "input",
        "keygen",
        "link",
        "meta",
        "param",
        "source",
        "track",
        "wbr"
    ],
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "prettier.printWidth": 120,
    "prettier.semi": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "npm.enableRunFromFolder": true,
    "turboConsoleLog.quote": "'",
    "terminal.integrated.defaultProfile.osx": "bash",
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explorer.confirmDelete": false,
    "yaml.schemas": {
        "file:///Users/mayerlench/.vscode/extensions/atlassian.atlascode-2.10.12/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "tailwindCSS.classAttributes": [
        "className",
        "class"
    ],
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "json.schemas": [],
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "editor.tokenColorCustomizations": {
        "[*Light*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#000"
                    }
                }
            ]
        },
        "[*Dark*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#fff"
                    }
                }
            ]
        },
        "textMateRules": []
    },
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "css.validate": false,
    "scss.validate": false,
    "less.validate": false,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false,
        "yaml": true,
        "dotenv": false,
        "typescript": true
    },
    "files.associations": {
        "*.embeddedhtml": "html",
        ".env*": "dotenv"
    },
    "dotenv.enableAutocloaking": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prisma.showPrismaDataPlatformNotification": false,
    "github.copilot.editor.enableAutoCompletions": true
}